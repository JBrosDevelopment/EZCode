<doc>
	<style name="Keyword" color="Yellow" fontStyle="Regular" />
	<style name="Afters" color="LightBlue" fontStyle="Regular" />
	<style name="Loop" color="DeepSkyBlue" fontStyle="Bold" />
	<style name="OneWords" color="Coral" fontStyle="Bold" />
	<style name="Comment" color="LightGreen" fontStyle="Regular" />
	<style name="Comment2" color="GreenYellow" fontStyle="Italic" />
	<rule style="Keyword">print |printRaw |write |writeRaw |object |image |label |font |move |scale |color |var |varRaw |textbox |multiLine |varInput|varSet |varEquals |varAddText |list |listRaw |writeToFile |sound |playFile |button |buttonClick </rule>
	<rule style="Loop">loop |end|: |if </rule>
	<rule style="OneWords">consoleClear|await |endBuild</rule>
	<rule style="Afters">~/|\/ |\* |\- |\+ | \!|\\_|\\n|\\!|&gt;|&lt;| ReadFile | AwaitKeyInput| KeyInput| ConsoleInput| Random | MousePosition| MouseClick| FromList| Key | Console | StickKey | await | AwaitKey | IntersectsWith | bold | italic | strikeout | underline | regular | FromTextBox | now |</rule>
	<rule style="Comment">\/\/.*$</rule>
	<rule style="Comment2">\/\/\/.*$</rule>
</doc>
