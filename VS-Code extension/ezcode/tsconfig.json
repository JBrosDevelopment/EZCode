{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "outDir": "out",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true
    },
    "exclude": ["node_modules", "out"]
  }
  